{
  "name": "elegant-tdd",
  "version": "1.0.0",
  "description": "Everything about TDD.",
  "main": "index.js",
  "devDependencies": {
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.32",
    "babel-core": "^6.26.0",
    "babel-plugin-espower": "^2.3.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "espower-typescript": "^8.1.1",
    "mocha": "^4.0.0",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.4",
    "sinon": "^4.0.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "mocha --require espower-typescript/guess --require babel-register test/**/*.test.ts",
    "report": "nyc npm test"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.test.js"
    ],
    "watermarks": {
      "lines": [
        85,
        95
      ],
      "statements": [
        85,
        95
      ],
      "functions": [
        85,
        95
      ],
      "branches": [
        85,
        95
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linesh-simplicity/elegant-TDD.git"
  },
  "keywords": [
    "tdd",
    "tasking",
    "test-driven-development",
    "deliberate-practice"
  ],
  "author": "Linesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linesh-simplicity/elegant-TDD/issues"
  },
  "homepage": "https://github.com/linesh-simplicity/elegant-TDD#readme"
}
